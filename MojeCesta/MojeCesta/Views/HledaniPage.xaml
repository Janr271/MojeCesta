<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MojeCesta.Views.HledaniPage"
             Title="Hledání">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Hledani"
                 Placeholder="Název zastávky"
                 TextChanged="Hledani_Changed"
                 Completed="Hledani_Completed"
                />
            <ListView x:Name="SeznamStanic"
                  ItemTapped="SeznamStanic_Tapped"
                />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>